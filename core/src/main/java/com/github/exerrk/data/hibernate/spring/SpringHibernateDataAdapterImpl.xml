<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<class name="com.github.exerrk.data.hibernate.spring.SpringHibernateDataAdapterImpl">
		<map-to xml="springHibernateDataAdapter"/>
		<field name="class" type="java.lang.Class" handler="com.github.exerrk.data.ClassNameFieldHandler">
			<bind-xml name="class" node="attribute"/>
		</field>
		<field name="name" type="string">
			<bind-xml name="name" node="element"/>
		</field>
		<field name="springConfig" type="string">
			<bind-xml name="springConfig" node="element"/>
		</field>
		<field name="beanId" type="string">
			<bind-xml name="beanId" node="element"/>
		</field>
	</class>
</mapping>
